{
  "name": "js-webpack-template",
  "version": "1.0.0",
  "description": "JavaScript Webpack Boilerplate is a template repository to quickly start a new bundled Vanilla JS project.",
  "homepage": "https://github.com/EmrahIso/js-webpack-template#readme",
  "bugs": {
    "url": "https://github.com/EmrahIso/js-webpack-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmrahIso/js-webpack-template.git"
  },
  "license": "ISC",
  "author": "Emrah Isovic",
  "type": "module",
  "main": "./src/js/index.js",
  "scripts": {
    "test:watch": "jest --watch *.test.js",
    "dev": "webpack serve --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "eslint": "npx eslint ./src/js/**/*.js --ignore-pattern node_modules/",
    "deploy": "git subtree push --prefix dist origin gh-pages"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@eslint/js": "^9.22.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "sass": "^1.86.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1"
  }
}
